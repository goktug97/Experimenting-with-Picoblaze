;random init
;load sA, 00110100'b
constant interrupt_pb1, 01
default_jump loop

load sA, 34
output sA, interrupt_pb1
; x^8 + x^6 + x^5 + x^4 + 1

load sC, sA
load s9, sA

sr0 sC
sr0 sC
xor s9, sC
sr0 sC
xor s9, sC
sr0 sC
xor s9, sC
and s9, 01

sr0 sA

load sB, 00

do_while_loop:
  sl0 s9
  add sB, 01
  compare sB, 07
  jump C, do_while_loop

or sA, s9

loop:
jump loop
